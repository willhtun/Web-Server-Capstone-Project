http {
  server {
    listen 80;
    root /usr/src/project;
    handler echo {
      url /echo;
    }
    handler static {
      location /static_files1;
      url /static1;
    }
    handler static {
      location /static_files2;
      url /static2;
    }
    handler status {
      url /status;
    }
  }
}